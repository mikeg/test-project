<div style="clear:both"></div>
<table cellpadding=2 cellspacing=2 border=1 width="100%" style="border-collapse: collapse">
  <tr bgcolor="##6699FF">
    <td>Student Name </td>
    <td>School</td>
    <td>Year Level</td>
    <td>Release status</td>
    <td>Date</td>
    <td>Accepting School</td>
  </tr>
  <%
  @students.each do |student|
  %>
  <tr>
    <td><%= link_to student.fullname, view_student_path(student) -%></td>
    <td><%= student.school.name %></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>

  </tr>
  <% end %>
</table>
</div>